#include "S32K144.h" /* include peripheral declarations S32K144 */

void delay (void)
{
unsigned long i=1000000;

do{}while (--i);
}

int main(void)
{
	PCC->PCCn[PCC_PORTD_INDEX]=0x40000000;
	PORTD->PCR[0]=0x00000100;
	//PORTD->PCR[15]=0x00000100;
	//PORTD->PCR[16]=0x00000100;
	PTD->PDDR=0x00018001;


        for(;;) {
        	PTD->PDOR=0x00018000;
        	delay();
        	PTD->PDOR=0x00018001;
        	delay();
        }

	return 0;
}
