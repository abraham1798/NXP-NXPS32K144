#include "S32K144.h" /* include peripheral declarations S32K144 */

void delay (void)
{
//velocity
unsigned long i=50000;
//slow unsigned long   i=300000;
do{}while (i--);
}

int main(void)
{
unsigned char i;
        //CGC Activate clock PCC
		PCC->PCCn[PCC_PORTB_INDEX]=0x40000000;

		//Select port (Mux)
		PORTB->PCR[17]=0x00000100;
		PORTB->PCR[16]=0x00000100;
		PORTB->PCR[15]=0x00000100;
		PORTB->PCR[14]=0x00000100;

		//Pin activate
		PTB->PDDR=0x0003C000;



        for(;;) {

        	int i;

        	  for (i = 1; i < 100; ++i)
        	  {
        		  PTB->PDOR=(1<<17);
        		  delay();
        		  PTB->PDOR=(1<<15);
        		  delay();
        		  PTB->PDOR=(1<<14);
        		  delay();
        		  PTB->PDOR=(1<<16);
        		  delay();
        	  }

        	  for (i = 1; i < 100; ++i)
        	          	  {
        	       PTB->PDOR=(1<<16);
        	       delay();
        	       PTB->PDOR=(1<<14);
        	       delay();
        	       PTB->PDOR=(1<<15);
        	       delay();
        	       PTB->PDOR=(1<<17);
        	       delay();
        	          	  }

        }

	return 0;
}
